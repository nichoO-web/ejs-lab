<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= category %> Menu</title>
</head>
<body>
    <%- include('./partials/nav') %>
    <h1><%= categoryName %> Menu</h1>
    <ul>
        <% menuItems.forEach((item, index) => { %>
          <li>
            $<%= item.price %> - <%= item.name %> - rating: <%= item.rating %>
            <p>
                - <%= item.details %>
            </p>
          </li>
        <% }) %>
    </ul>
</body>
</html>